{
  "name": "parquets",
  "description": "TypeScript implementation of the Parquet file format, based on parquet.js",
  "main": "lib/index.js",
  "version": "0.8.2",
  "homepage": "https://github.com/kbajalc/parquets",
  "author": "kbajalc@gmail.com",
  "license": "MIT",
  "keywords": [
    "dremel",
    "parquet"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kbajalc/parquets.git"
  },
  "dependencies": {
    "brotli": "^1.3.0",
    "bson": "^1.0.4",
    "int53": "^0.2.4",
    "lz4": "^0.5.3",
    "lzo": "^0.4.0",
    "object-stream": "0.0.1",
    "snappyjs": "^0.6.0",
    "thrift": "^0.10.0",
    "varint": "^5.0.0"
  },
  "devDependencies": {
    "@creditkarma/thrift-typescript": "^2.0.0",
    "@types/bson": "^1.0.9",
    "@types/node": "^10.3.4",
    "chai": "^4.1.2",
    "mocha": "^3.5.3"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "rm -rf ./lib ; tsc -p .",
    "watch": "rm -rf ./lib ; tsc -p . --watch",
    "test": "mocha",
    "gen": "npx thrift-typescript --target apache --rootDir . --sourceDir . --outDir codegen parquet.thrift",
    "thrift": "thrift --gen js:node parquet.thrift && thrift --gen js:ts parquet.thrift"
  },
  "engines": {
    "node": ">=7.6"
  }
}